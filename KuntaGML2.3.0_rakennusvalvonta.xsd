			<xs:extension base="gml:AbstractFeatureType"> 
				<xs:sequence>
					<xs:element name="Kerrosluku" type="xs:int"/>
					<xs:element name="Kayttoonottopaivamaara" type="xs:string"/>
					<xs:element name="PysyvaRakennustunnus" type="xs:string"/>
