	<!-- Kohdejoukko: Luvat -->
	<xs:element name="Rakennusvalvonta" type="rakval:rakennusvalvontaType" substitutionGroup="gml:_Feature">
		<xs:annotation>
			<xs:documentation>Skeeman juurielementti</xs:documentation>
		</xs:annotation>
	</xs:element>
	<!-- uusi -->
	<xs:complexType name="RakennuskohdeType" >
		<xs:complexContent>
			<xs:extension base="rakval:AbstractRakvalKohde"> 
				<xs:sequence>
					<xs:element name="kuvaus" type="xs:string"/>
					<xs:element name="nimi" type="xs:string"/>
					<xs:element name="rakennuskohteenTiedonLaji">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="1 - Suunnitelman mukainen tieto"/>
								<xs:enumeration value="2 - Toteuman mukainen tieto"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="suunniteltuPurkamipaivamaara" type="xs:date" />
					<xs:element name="valiaikainen" type="xs:boolean" />
					<xs:element name="kiinteistoTaiMaaraAla" type="yht:KiinteistoTaiMaaraAlaType"/>
					<xs:element name="liiteasiakirja" type="yht:Asiakirja"/>
					<xs:element name="rakennuksenTietomalli" type="yht:AsiakirjaType"/>
					<xs:element name="rakennuskohteenOmistaja" type="yht:rakennuskohteenOmistajaType"/>
					<xs:element name="rakennuskohteenOsoite" type="yht:OsoiteType"/>
					<xs:element name="rakennuspaikka" type="yht:RakennuspaikkaType"/>
					<xs:element name="sijainti" type="yht:SijaintiType"/>
					<xs:element name="verkostoliittyma" type="rakval:VerkostoliittymatType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RakennelmaType">
		<xs:complexContent>
			<xs:extension base="rakval:AbstractRakvalKohde"> 
				<xs:sequence>
					<xs:element name="kerrosluku" type="xs:int" minOccurs="0" />
					<!-- attribuutteja vanhasta -->
					<xs:element name="koostuu" type="rakval:RakennelmanOsaType" minOccurs="1"/>
					<xs:element name="ylaluokka" type="rakval:RakennuskohdeType" minOccurs="1"/>
					<xs:element name="kuvaus">
						<xs:complexType>
							<xs:choice>
								<xs:element name="kuvaus" type="xs:string"/>
								<xs:element name="liite" type="yht:RakennusvalvontaLiiteType"/>
							</xs:choice>
						</xs:complexType>
					</xs:element>
					<xs:element name="tunnus" type="yht:RakennustunnusType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>vastaava kuin rakennustunnus</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="kokoontumistilanHenkilomaara" type="xs:int" minOccurs="0"/>
					<xs:element name="tilapainenRakennelmaKytkin" type="xs:boolean" minOccurs="0"/>
					<xs:element name="tilapainenRakennelmavoimassaPvm" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
