								<xs:enumeration value="04 - Toimistorakennukset"/>
								<xs:enumeration value="05 - Liikenteen rakennukset"/>
								<xs:enumeration value="06 - Hoitoalan rakennukset"/>
								<xs:enumeration value="07 - Kokoontumisrakennukset"/>
								<xs:enumeration value="08 - Opetusrakennukset"/>
								<xs:enumeration value="09 - Teollisuuden ja kaivannaistoiminnan rakennukset"/>
								<xs:enumeration value="10 - Energiahuoltorakennukset"/>
								<xs:enumeration value="11 - Yhdyskuntatekniikan rakennukset"/>
								<xs:enumeration value="12 - Varastorakennukset"/>
								<xs:enumeration value="13 - Pelastustoimen rakennukset"/>
								<xs:enumeration value="14 - Maatalousrakennukset ja eläinsuojat"/>
								<xs:enumeration value="19 - Muut rakennukset"/>
