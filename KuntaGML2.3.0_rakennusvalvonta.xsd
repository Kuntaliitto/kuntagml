								<xs:enumeration value="015 - Kasvihuone"/>
								<xs:enumeration value="016 - Katos/pergola"/>
								<xs:enumeration value="017 - Katsomo"/>
								<xs:enumeration value="018 - Katumainostaulu"/>
								<xs:enumeration value="019 - Kelluva rakennelma"/>
								<xs:enumeration value="020 - Kierratyspiste"/>
								<xs:enumeration value="021 - Kioski"/>
								<xs:enumeration value="022 - Kolmiopilari"/>
								<xs:enumeration value="023 - Laituri"/>
								<xs:enumeration value="024 - Lastauslaituri"/>
								<xs:enumeration value="025 - Liikuteltava grillikioski"/>
