	<xs:complexType name="RakennusluvatType">
		<!-- tahan sisaltöa? -->
	</xs:complexType>
	<xs:complexType name="VanhaRakennusluvatType"> <!-- vanha luokka -->
		<xs:annotation>
			<xs:documentation>Luvat</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="rakennuslupa" type="yht:LuvanTunnisteTiedotType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Rakennusluvan tiedot</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractRakvalKohde" abstract="true">
		<xs:annotation>
			<xs:documentation>Kohde josta periytetaan kaikki kohteet, joilla on sijaintitieto</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="yht:AbstractPaikkatietopalveluKohde">
				<xs:sequence>
					<xs:element name="sijaintitieto">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Sijainti" type="yht:RakennusvalvontaSijaintiType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VarusteType"> <!-- uusi luokka -->
		<xs:sequence>
			<xs:element name="varusteenLajiRakennus">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="01 - Sähkö"/>
						<xs:enumeration value="02 - Kaasu"/>
						<xs:enumeration value="03 - Jätevesiviemäri"/>
						<xs:enumeration value="04 - Vesijohto"/>
						<xs:enumeration value="05 - Lämmin vesi"/>
						<xs:enumeration value="06 - Koneellinen ilmastointi"/>
						<xs:enumeration value="07 - Aurinkopaneeli"/>
						<xs:enumeration value="08 - Hissi"/>
						<xs:enumeration value="09 - Uima-allas"/>
						<xs:enumeration value="10 - Sauna"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="varusteenLajiHuoneisto">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="01 - WC"/>
						<xs:enumeration value="02 - Suihku"/>
						<xs:enumeration value="03 - Sauna"/>
						<xs:enumeration value="04 - Parveke"/>
						<xs:enumeration value="05 - Lämmin vesi"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="varusteenLukumaara" type="xs:int"/>
			<xs:element name="varusteenTyyppi">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="01 - Rakennuskohteen varuste"/>
						<xs:enumeration value="02 - Rakennuksen varuste"/>
						<xs:enumeration value="03 - Tilan varuste"/>
						<xs:enumeration value="04 - Huoneiston varuste"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HuoneistonVarusteetType"> <!-- vanha luokka, tarpeeton vrt VarusteType -->
		<xs:sequence>
			<xs:element name="WCKytkin" type="xs:boolean"/>
			<xs:element name="ammeTaiSuihkuKytkin" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>yhdistetaan suihkun kanssa</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="saunaKytkin" type="xs:boolean"/>
			<xs:element name="parvekeTaiTerassiKytkin" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Yhdistetaan terassin kanssa</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lamminvesiKytkin" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UlkokuorenTiedotType"> <!-- uusi luokka -->
		<xs:sequence>
			<xs:element name="kerrosala" type="xs:decimal"/>
			<xs:element name="kerrosluku" type="xs:integer"/>
			<xs:element name="kokonaisala" type="xs:decimal"/>
			<xs:element name="korkeus" type="xs:decimal"/>
			<xs:element name="lentoeste" type="xs:boolean"/>
			<xs:element name="rakennusoikeudellinenKerrosala" type="xs:decimal"/>
			<xs:element name="suhdeMaanPintaan">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="01 - Kokonaan pinnan ylla"/>
						<xs:enumeration value="02 - Olennaisesti kokonaan pinnan ylla"/>
						<xs:enumeration value="03 - Olennaisesti pinnan ylla ja alla"/>
						<xs:enumeration value="04 - Olennaisesti pinnan alla"/>
						<xs:enumeration value="05 - Kokonaan pinnan alla"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tilavuus" type="xs:integer"/>
			<xs:element name="muoto" type="yht:SijaintiType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TilastotiedotType"> <!-- vanha luokka -->
		<xs:sequence>
			<xs:element name="uusiaAsuntojaLkm" type="xs:int" minOccurs="0"/>
			<xs:element name="uusiaRakennuksiaLkm" type="xs:int" minOccurs="0"/>
			<xs:element name="paloluokka" type="xs:string" minOccurs="0"/>
			<xs:element name="tilavuus" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Uusi tilavuus</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VakuusType"> <!-- vanha luokka -->
		<xs:sequence>
			<xs:element name="vakuudenLaji" type="xs:string" minOccurs="0"/>
			<xs:element name="voimassaolopvm" type="xs:date"/>
			<xs:element name="vakuudenmaara" type="xs:int" nillable="true"/>
			<xs:element name="vakuuspaatospykala" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="KatselmuksenLajiType">  <!-- korvattu vanha koodisto uudella -->
		<xs:restriction base="xs:string">
			<xs:enumeration value="01 - Aloittamisilmoitus"/>
			<xs:enumeration value="02 - Aloituskokous"/>
			<xs:enumeration value="03 - Pohjakatselmus"/>
			<xs:enumeration value="04 - Sijaintikatselmus"/>
			<xs:enumeration value="05 - Rakennekatselmus"/>
			<xs:enumeration value="06 - Lampölaitteiden katselmus"/>
			<xs:enumeration value="07 - Vesilaitteiden katselmus"/>
			<xs:enumeration value="08 - Ilmanvaihtolaitteiden katselmus"/>
			<xs:enumeration value="09 - Osittainen loppukatselmus"/>
			<xs:enumeration value="10 - Loppukatselmus"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- uudet koodistot Katselmuksen lopullisuus ja Katselmuksen tilanne -->
	<xs:simpleType name="KatselmuksenLopullisuusType">  <!-- uusi -->
		<xs:restriction base="xs:string">
			<xs:enumeration value="01 - Katselmus suoritettu osittaisena"/>
			<xs:enumeration value="02 - Katselmus on lopullinen"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KatselmuksenTilanneType">  <!-- uusi -->
		<xs:restriction base="xs:string">
			<xs:enumeration value="01 - Hyvaksytty"/>
			<xs:enumeration value="02 - Korjattava"/>
			<xs:enumeration value="03 - Hyvaksytty ehdolla"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="HuomautusType"> <!-- vanha luokka -->
		<xs:sequence>
			<xs:element name="kuvaus" type="xs:string">
				<xs:annotation>
					<xs:documentation>Huomautuksen kuvaus</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maaraAika" type="xs:date" minOccurs="0"/>
			<xs:element name="toteamisHetki" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Huomautus/velvoite on toteutunut</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="toteaja" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HuomautuksetType">
		<xs:sequence>
			<xs:element name="huomautus" type="rakval:HuomautusType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Katselmuksessa annetut huomautus/velvoite</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- uusi, korvaa alla olevan -->
