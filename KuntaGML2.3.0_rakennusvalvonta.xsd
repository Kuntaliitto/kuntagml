								<xs:enumeration value="030 - Markiisi"/>
								<xs:enumeration value="031 - Masto"/>
								<xs:enumeration value="032 - Muu kaytto"/>
								<xs:enumeration value="033 - Muu rakennelma"/>
								<xs:enumeration value="034 - Muu toimenpide"/>
								<xs:enumeration value="035 - Muu vesirajalaite"/>
