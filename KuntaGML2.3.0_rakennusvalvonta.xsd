					<xs:element name="VaestonsuojienLukumaara" type="xs:int"/>
					<xs:element name="VaestonsuojienSuojapaikkojenMaara" type="xs:int"/>
					<xs:element name="koostuu" type="rakval:ValmisRakennuksenOsaType" minOccurs="1"/>
					<xs:element name="ylaluokka" type="rakval:RakennuskohdeType" minOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VanhaValmisRakennusType"> <!-- vanha luokka, vrt ValmisRakennusType -->
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="rakennustieto">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Rakennus" type="rakval:RakennusType"/>
