	<xs:complexType name="AbstractPaikkatietopalveluKohde" abstract="true">
		<xs:annotation>
			<xs:documentation>Kohde josta periytetaan kaikki kohteet</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="metatieto" type="gml:MetaDataPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Kohteen metatiedot. Suositeltavat kentat ovat: sijaintitiedonluontitapa, tarkkuus, datan_luoja, luonti_pvm, muokkaaja, muokkaus_pvm, omistaja, lahteen_pvm, lahteen_tyyppi, mittausera, lisatieto_linkki</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="yksilointitieto" type="xs:string">
						<xs:annotation>
							<xs:documentation>Kohteen lahtotietojarjestelman  tvs. mukainen yksilointitieto </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="alkuHetki" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>Kohteen luontipaivamaara</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="loppuHetki" type="xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Milloin kohde on poistettu. Taman avulla voidaan tunnistaa poistetut kohteet, jos jarjestelmat tukevat historiatietojen tallentamista</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AsiakirjaType"> <!-- uusi -->
		<xs:complexContent>
			<xs:extension base="yht:AbstractPaikkatietopalveluKohde">
				<xs:sequence>
					<xs:simpleType name="asiakirjanLaji">
						<xs:restriction base="xs:string">
							<xs:enumeration value="01 - Hakemus"/>
							<xs:enumeration value="02 - Kartta"/>
							<xs:enumeration value="03 - Kirje"/>
							<xs:enumeration value="04 - Kuulutus"/>
							<xs:enumeration value="05 - Lausunto"/>
							<xs:enumeration value="06 - Mielipide"/>
							<xs:enumeration value="07 - Muistio"/>
							<xs:enumeration value="08 - Muistutus"/>
							<xs:enumeration value="09 - Päätös"/>
							<xs:enumeration value="10 - Pöytäkirja"/>
							<xs:enumeration value="11 - Rakennusjärjestys"/>
							<xs:enumeration value="12 - Selvitys"/>
							<xs:enumeration value="13 - Suunnitelma"/>
							<xs:enumeration value="14 - Valitus"/>
							<xs:enumeration value="15 - Vastine"/>
							<xs:enumeration value="99 - Muu"/>
						</xs:restriction>
					</xs:simpleType>
					<xs:element name="asiakirjanLajinTarkenne" type="xs:string"/>
					<xs:element name="asiakirjanPaivamaara" type="xs:date"/>
					<xs:simpleType name="henkilotietosisalto">
						<xs:restriction base="xs:string">
							<xs:enumeration value="1 - Ei sisällä henkilötietoja"/>
							<xs:enumeration value="2 - Sisältää nimitietoja"/>
							<xs:enumeration value="1 - Sisältää henkilötietoa, josta henkilö tunnistettavissa"/>
						</xs:restriction>
					</xs:simpleType>
					<xs:simpleType name="julkisuusluokka">
						<xs:restriction base="xs:string">
							<xs:enumeration value="1 - Julkinen asiakirja"/>
							<xs:enumeration value="2 - Sisältää turvaluokiteltua tietoa"/>
							<xs:enumeration value="3 - Sisältää salassapidettävää tietoa"/>
							<xs:enumeration value="4 - Tiedon jakaminen muille kuin viranomaisille vaatii tarkistamista"/>
						</xs:restriction>
					</xs:simpleType>
					<xs:simpleType name="kieli">
						<xs:restriction base="xs:string">
							<xs:enumeration value="fi - suomi"/>
							<xs:enumeration value="sv - ruotsi"/>
							<xs:enumeration value="en - englanti"/>
							<xs:enumeration value="smn - inarinsaame"/>
							<xs:enumeration value="sms - koltansaame"/>
							<xs:enumeration value="se - pohjoissaame"/>
						</xs:restriction>
					</xs:simpleType>
					<xs:element name="nimi" type="xs:string"/>
					<xs:element name="saapumispaivamaara" type="xs:date"/>
					<xs:element name="saavutettavuus" type="xs:boolean"/>
					<xs:simpleType name="sailytysaika">
						<xs:restriction base="xs:string">
							<xs:enumeration value="01 - Säilytetään pysyvästi"/>
							<xs:enumeration value="02 - Säilytetään määräajan"/>
						</xs:restriction>
					</xs:simpleType>
					<xs:element name="tiedosto" type="xs:base64Binary"/>
					<xs:element name="vahvistuspaivamaara" type="xs:date"/>
					<xs:element name="asiasana" type="yht:AsiasanaType"/>
					<xs:element name="laatija" type="yht:ToimijaType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AsiasanaType"> <!-- uusi -->
		<xs:complexContent>
			<xs:extension base="yht:AbstractPaikkatietopalveluKohde">
				<xs:sequence>
