					<xs:element name="pintavedenlaatuhavaintotieto" type="yms:PintaVesiType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="pohjavedenlaatuhavainnot" type="yms:PohjavesiType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
