<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:yms="http://www.kuntatietopalvelu.fi/gml/ymparisto/ympariston_tila_ja_seuranta" xmlns:gml="http://www.opengis.net/gml" xmlns:yht="http://www.kuntatietopalvelu.fi/gml/yhteiset" targetNamespace="http://www.kuntatietopalvelu.fi/gml/ymparisto/ympariston_tila_ja_seuranta" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.1.6">
	<xs:import namespace="http://www.opengis.net/gml" schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd"/>
