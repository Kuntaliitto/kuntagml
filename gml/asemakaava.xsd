								<xs:enumeration value="erillinen tonttijako"/>
								<xs:enumeration value="kaavan sisältyy sitova tonttijako"/>
								<xs:enumeration value="kaavaan sisältyy ohjeellinen tonttijako"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="tonttijaontunnus" type="xs:string" minOccurs="0"/>
					<xs:element name="kaavallinenOlotila" minOccurs="0">
