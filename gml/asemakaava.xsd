							<xs:documentation>Kaavan yksilöivä tunnus, johon kaavayksikkö kuuluu</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="kaavayksikonTunnus" type="xs:string" minOccurs="0"/>
