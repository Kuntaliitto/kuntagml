						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="ei tiedossa"/>
								<xs:enumeration value="keinoyksikkö (maarekisterialue)"/>
								<xs:enumeration value="keinokaavayksikkö (yleisen alueen lisäosa)"/>
								<xs:enumeration value="rekisteröity yleinen alue"/>
								<xs:enumeration value="yleisen alueen lisäosa (rekisterissä)"/>
								<xs:enumeration value="muun kuin korttelialueen yksikkö"/>
								<xs:enumeration value="tonttirekisteritontti"/>
								<xs:enumeration value="muodostusluetteloon merkitty tontti"/>
								<xs:enumeration value="hyväksytyn tonttijaon mukainen tontti"/>
								<xs:enumeration value="vireillä olevan tonttijaonmuutoksen mukainen tontti"/>
								<xs:enumeration value="vireillä olevan tonttijaon/-muutoksen mukainen tontti"/>
								<xs:enumeration value="ohjeellinen kaavatontti"/>
								<xs:enumeration value="korttelin osa"/>
								<xs:enumeration value="kortteli"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="tonttijaonlaji" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
