							<xs:restriction base="xs:string">
								<xs:enumeration value="asemakaavayksikön rajojen muutos, vireillä"/>
								<xs:enumeration value="asemakaavan muutos, lainvoimainen"/>
								<xs:enumeration value="asemakaavan muutos, vireillä"/>
								<xs:enumeration value="ensimmäinen asemakaava, lainvoimainen"/>
								<xs:enumeration value="ensimmäinen asemakaava, vireillä"/>
								<xs:enumeration value="asemakaava, lainvoimainen"/>
								<xs:enumeration value="asemakaava, vireillä"/>
								<xs:enumeration value="ei tiedossa"/>
								<xs:enumeration value="rakennuskaava, lainvoimainen"/>
								<xs:enumeration value="rakennuskaava, vireillä"/>
								<xs:enumeration value="rantakaava, lainvoimainen"/>
								<xs:enumeration value="rantakaava, vireillä"/>
								<xs:enumeration value="vahvistettu yleiskaava, lainvoimainen"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="voimassaolo" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="ei tiedossa"/>
								<xs:enumeration value="muu"/>
								<xs:enumeration value="kumottu"/>
								<xs:enumeration value="kiinteistö kumottu"/>
								<xs:enumeration value="voimassa"/>
								<xs:enumeration value="vireillä"/>
								<xs:enumeration value="numeronvaraus"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="rakennusoikeus" minOccurs="0">
