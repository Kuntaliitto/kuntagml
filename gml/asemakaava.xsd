							<xs:sequence>
								<xs:element name="perusmaara" type="xs:string" minOccurs="0"/>
								<xs:element name="kaytetty" type="xs:string" minOccurs="0"/>
								<xs:element name="lisarakennusoikeus" type="xs:string" minOccurs="0"/>
								<xs:element name="kellaritilat" type="xs:string" minOccurs="0"/>
								<xs:element name="ullakkotilat" type="xs:string" minOccurs="0"/>
								<xs:element name="kerrostenLkm" type="xs:string" minOccurs="0"/>
								<xs:element name="maanalaistenTilojenLkm" type="xs:string" minOccurs="0"/>
								<xs:element name="kerrosjaonMaaraytymistapa" type="xs:string" minOccurs="0"/>
								<xs:element name="lisatehokkuus" type="xs:string" minOccurs="0"/>
								<xs:element name="tehokkuusluku" type="xs:string" minOccurs="0"/>
