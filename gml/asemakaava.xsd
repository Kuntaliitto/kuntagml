					<xs:element name="muodostuminen" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TIeto siitä mistä kiinteistä muodostuu ja kuinka suuri osa ko. kiinteistöstä</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence maxOccurs="unbounded">
								<xs:element name="kiinteisto" type="xs:string"/>
								<xs:element name="pintaAla" type="xs:decimal" nillable="true"/>
								<xs:element name="kokoKiinteistoKytkin"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="pintaAla" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Kaavayksikön pinta-ala</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="pintaAlanMaarittamistapa" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Tieto siitä miten pinta-ala on määritetty</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="geodeettinen"/>
								<xs:enumeration value="ei tiedossa"/>
								<xs:enumeration value="digitoitu"/>
								<xs:enumeration value="graafinen"/>
								<xs:enumeration value="muu"/>
								<xs:enumeration value="pinta-ala muuttunut kiinteistön kautta"/>
								<xs:enumeration value="pinta-alaa ei ole määritetty"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="sijainti" type="akaava:AlueSijaintiType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Aluemuoto ensisijainen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="rakennettuKytkin" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Onko kyseessä uusi(true) vai vanha rakennuspaikka(false)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lisatieto" type="xs:string" minOccurs="0"/>
					<xs:element name="kayttotarkoitus" type="xs:string" minOccurs="0"/>
					<xs:element name="osoite" type="yht:OsoiteType" minOccurs="0"/>
					<xs:element name="korotettuKiinteistoveroalkuPvm" type="xs:date" minOccurs="0"/>
					<xs:element name="korotettuKiinteistoveroloppuPvm " type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="KaavayksikotType">
		<xs:sequence>
			<xs:element ref="akaava:Kaavayksikko" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Kaavayksikko" type="akaava:KaavayksikkoType" substitutionGroup="akaava:_Kaavakohde"/>
