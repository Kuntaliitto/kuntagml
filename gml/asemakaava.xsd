			<xs:enumeration value="29.6.1943 asetus asemakaavoista"/>
			<xs:enumeration value="30.9.1959 Sisäasiainministeriön asetus"/>
			<xs:enumeration value="15.1.1980 Sisäasiainministeriön asetus"/>
