										<xs:documentation>Tieto siitä miten rakennusoikeus on määritety</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="ei tiedossa"/>
											<xs:enumeration value="ei rakennusoikeutta"/>
											<xs:enumeration value="tehokkuusluku"/>
											<xs:enumeration value="kerrosneliömetreinä"/>
											<xs:enumeration value="rakennusjärjestyksen perusteella"/>
											<xs:enumeration value="kaavamääräysten perusteella"/>
											<xs:enumeration value="mitattu ja laskettu kaavakartalta"/>
											<xs:enumeration value="rakennusvalvontaviranomaisen tulkinta/tehokkuusluku"/>
											<xs:enumeration value="rakennusvalvontaviranoimaisen tulkinta/k-m2"/>
											<xs:enumeration value="ei voida määrittää"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
