								<xs:element ref="kanta:_Vesistokohde" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="luonnonsuojelukohteet" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
